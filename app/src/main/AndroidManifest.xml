<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ahf.edu.tesoem.itics.intent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OnceActivity"></activity>
        <activity android:name=".DiezActivity" />
        <activity android:name=".NueveActivity" />
        <activity android:name=".OchoActivity" />
        <activity android:name=".SieteActivity" />
        <activity android:name=".SeisActivity" />
        <activity android:name=".CincoActivity" />
        <activity android:name=".CuatroActivity" />
        <activity android:name=".TresActivity" />
        <activity android:name=".SegundoActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>